{
  "CalendarMcp": {
    "Accounts": [
      {
        "Id": "example-m365",
        "DisplayName": "Example M365 Account",
        "Provider": "microsoft365",
        "Domains": ["example.com"],
        "Enabled": false,
        "Priority": 1,
        "ProviderConfig": {
          "TenantId": "your-tenant-id",
          "ClientId": "your-client-id"
        }
      },
      {
        "Id": "example-google",
        "DisplayName": "Example Google Account",
        "Provider": "google",
        "Domains": ["gmail.com"],
        "Enabled": false,
        "Priority": 0,
        "ProviderConfig": {
          "ClientId": "your-google-client-id",
          "ClientSecret": "your-google-client-secret"
        }
      }
    ],
    "Telemetry": {
      "Enabled": true,
      "MinimumLevel": "Information"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
